# mount point	fstype		device			[device2]

/boot			emmc		/dev/bootimg
/recovery		emmc		/dev/recovery
/system			ext4		/dev/block/mmcblk0p5
/cache			ext4		/dev/block/mmcblk0p6
/data			ext4		/dev/block/mmcblk0p7
/internal_sd	vfat		/dev/block/mmcblk0p8						flags=display="Internal SD";settingsstorage;wipeingui
/external_sd	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Micro SD";storage;wipeingui;removable
